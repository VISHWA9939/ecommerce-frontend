{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "dist/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "dist/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "dist/manifest.json"
    },
    {
      "src": "/asset-manifest.json",
      "dest": "dist/asset-manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
